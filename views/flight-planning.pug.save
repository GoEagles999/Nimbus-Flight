title= title
link(rel='stylesheet', href='/stylesheets/map.css')
link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css", integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u", crossorigin="anonymous")
script(src='https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js')
script(src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js", integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=", crossorigin="anonymous")
script(src='/javascripts/analytics.js')
body
  div#container
    //add on-focus value = '' to clear default value of input field
    input#pac-input.pac-controls(type='text', placeholder='Enter location or coordinates in the form of "latitude, longitude".' style='width:600px;')
   -controls
    div.buttons#add-btn(onclick="showMenu('add-box')")
    div.buttons#sync-data-btn(onclick="showMenu('sync-data-box')")
    div.buttons#user-info-btn(onclick="showMenu('user-info-box')")
    div#add-box.boxes(hidden)
      div#addNewFlight(style='display:block; padding:10px 10px 10px 30px;border-bottom:1px solid grey;' hidden)
        p Add New Flight
    div#sync-data-box.boxes(hidden)
      div#flightPlans(style='display:block; padding:10px 10px 10px 30px;border-bottom:1px solid grey;' hidden)
        p Flight Plans
      div#models(style='display:block; padding:10px 10px 10px 30px;border-bottom:1px solid grey;' hidden)
        p Telemetry Logs
    div#user-info-box.boxes(hidden)
      div#myAccount(style='display:block; padding:10px 10px 10px 30px;' onclick='window.location="https://nimbusflight.io/settings";' hidden)
        p Settings
    div#map
script(src='/javascripts/flight-planning.js')
script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAKe685MWNqhj6-PL3plap1_uCQrHVQYTE&libraries=places&callback=initMap' async defer)
